<h1 class="text-center">我发表的文章</h1>

<table class="table">
  <thead>
    <tr>
      <th>#</th>
      <th>文章内容</th>
      <th>发表时间</th>
      <th>管理选项</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.id %></td>
        <td><%= post.content %></td>
        <td><%= post.created_at %></td>
        <td>
          <%= link_to("编缉文章", edit_group_post_path(post.group,post), class: :"btn btn-success btn-xs") %>
          <%= link_to("删除文章", group_post_path(post.group, post), method: :delete, class: :"btn btn-danger btn-xs") %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
